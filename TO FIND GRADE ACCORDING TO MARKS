#include <stdio.h>
int main()
{
    int pps,chem,mech,math,gen_attitude;
    float per;
    /*Input marks of five subjects from user*/
    printf("Enter five subject marks :");
    scanf("%d%d%d%d%d",&pps,&chem,&mech,&math,&gen_attitude);
    /*calculate percentage*/
    per=(pps+chem+mech+math+gen_attitude)/5.0;
    printf("percentage=%.2f\n",per);
    /*Find grade according to the percentage*/
    if(per>=90)
    {
        printf("GRADE A");
    }
        else if(per>=80)
        {
            printf("GRADE B");
        }
        else if(per>=70)
        {
            printf("GRADE C");
        }
        else if(per>=60)
        {
            printf("GRADE D");
        }
        else if(per>=50)
        {
            printf("GRADE E");
        }
        else if(per>=40)
        {
            printf("GRADE F");
        }
        return 0;
    }
